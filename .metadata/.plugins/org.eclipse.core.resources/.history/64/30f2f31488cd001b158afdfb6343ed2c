
public class Address 
{
	String streetNumber;
	String unitNumber;
	String state;
	int zipCode;
	String email;
	
	//the Block of code below will be our set methods.
	public void setStreetNumber(String streetNumber) 
	{
		this.streetNumber = streetNumber;
	}
	
	public void setUnitNumber(String unitNumber) 
	{
		this.unitNumber = unitNumber;
	}
	
	public void setState(String state) 
	{
		this.state = state;
	}
	
	public void setZipCode(int zipCode) 
	{
		this.zipCode = zipCode;
	}
	
	public void setEmail(String email) 
	{
		this.email = email;
	}
	//end of SET methods
	
	
	//The Block of code below will do the following
	//Will return the variables they are assigned to, they will also print out the variable name for testing.
	public String getStreetNumber() 
	{
		System.out.println(streetNumber);
		return streetNumber;
	}
	public String getUnitNumber() 
	{
		System.out.println(unitNumber);
		return unitNumber;
	}
	public String getState() 
	{
		System.out.println(state);
		return state;
	}
	public int getZipCode() 
	{
		System.out.println(zipCode);
		return zipCode;
	}
	public String getEmail() 
	{
		System.out.println(email);
		return email;
	}
	//End of GET methods
	
	
	
}
