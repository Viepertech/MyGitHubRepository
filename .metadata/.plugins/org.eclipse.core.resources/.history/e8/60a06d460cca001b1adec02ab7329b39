//this program will take two int values input by the user and add the sum, printing the output.

import java.util.Scanner;

public class Summer2021 
{
	public static void main(String[] args)
	{
		int num1, num2;
		Scanner input = new Scanner(System.in);
		
		num1 = input.nextInt();
		num2 = input.nextInt();
		
		int sum = num1 + num2;
		
		
		System.out.println(sum);

	}
	
}
